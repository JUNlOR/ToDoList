{
  "index.html": {
    "src": "src/html/index.html",
    "dest": "target/index.html"
  },
  "view.html": {
    "expand": true,
    "cwd": "src/html/view/",
    "src": "**/*",
    "dest": "target/view/"
  },
  "js": {
    "expand": true,
    "cwd": "src/",
    "src": "js/**",
    "dest": "target/js",
    "flatten": true,
    "filter": "isFile"
  }
}